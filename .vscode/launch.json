{
    "configurations": [
        {
            "name": "Run this file",
            "type": "node",
            "request": "launch",

            "args": ["--import", "./scripts/loader-register.js", "${file}"],

            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "skipFiles": [
                "<node_internals>/**",
                "${workspaceFolder}/node_modules/**",
            ],
        }
    ],
}
